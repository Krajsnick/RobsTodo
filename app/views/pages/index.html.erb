<% provide(:title, "RobsTodo") %>
<div ng-controller="TodoCtrl">
  <h2 class="center">RobsTodo</h2>

  <form ng-submit="addTodo()">
    <input type="text" ng-model="entry.text" placeholder="What todo...?">
  </form>

  <table>
    <tr ng-repeat="todo in todos">
      <td>{{todo.text}}</td>
      <td>{{todo.done}}</td>
    </tr>
  </table>
{{entry.text}}
{{someangular}}

</div>